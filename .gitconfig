[alias]
  df = diff --word-diff=color
[color]
  status = always
[color "status"]
  added = green
  changed = red bold
  unmerged = red bold
  untracked = magenta
[core]
  editor = vi
  pager = less
[diff]
  wordRegex = [a-zA-Z0-9_-]+|.
[init]
	templatedir = ~/.git-templates
[push]
  default = simple
